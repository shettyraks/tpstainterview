{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "tpstaadf"
		},
		"adls_rssfeeds_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_rssfeeds'"
		},
		"azsql_datamart_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azsql_datamart'"
		},
		"adls_rssfeeds_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tpstainterview001.dfs.core.windows.net"
		},
		"http_fuelprice_rssfeeds_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.fuelwatch.wa.gov.au/fuelwatch/fuelWatchRSS?"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "tpstaadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adls_rssfeeds_accountKey": {
						"value": "[parameters('adls_rssfeeds_accountKey')]"
					},
					"azsql_datamart_connectionString": {
						"value": "[parameters('azsql_datamart_connectionString')]"
					},
					"adls_rssfeeds_properties_typeProperties_url": {
						"value": "[parameters('adls_rssfeeds_properties_typeProperties_url')]"
					},
					"http_fuelprice_rssfeeds_properties_typeProperties_url": {
						"value": "[parameters('http_fuelprice_rssfeeds_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}